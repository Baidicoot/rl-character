{
  "plot_title": "Choice Task Analysis",
  "figure_size": [12, 8],
  "layout": {
    "rows": 2,
    "cols": 2
  },
  "subplots": [
    {
      "title": "Broken Tests - All Choices",
      "position": [0, 0],
      "x_labels": ["broken_tests"],
      "metrics": [
        {
          "name": "clean_rate",
          "label": "clean choice",
          "style": {"alpha": 0.7}
        },
        {
          "name": "flag_rate",
          "label": "flag choice",
          "style": {"alpha": 0.7, "hatch": "//"}
        },
        {
          "name": "hack_rate",
          "label": "hack choice",
          "style": {"alpha": 0.7, "hatch": "\\\\"}
        }
      ],
      "data_sources": [
        {
          "x_index": 0,
          "file_pattern": "choice_broken_tests_{model}.jsonl",
          "metrics": {
            "clean_rate": "choice_rates.clean",
            "flag_rate": "choice_rates.flag",
            "hack_rate": "choice_rates.hack"
          }
        }
      ],
      "y_limit": [0, 1]
    },
    {
      "title": "Clean Tests - All Choices",
      "position": [0, 1],
      "x_labels": ["clean_tests"],
      "metrics": [
        {
          "name": "clean_rate",
          "label": "clean choice",
          "style": {"alpha": 0.7}
        },
        {
          "name": "flag_rate",
          "label": "flag choice",
          "style": {"alpha": 0.7, "hatch": "//"}
        },
        {
          "name": "hack_rate",
          "label": "hack choice",
          "style": {"alpha": 0.7, "hatch": "\\\\"}
        }
      ],
      "data_sources": [
        {
          "x_index": 0,
          "file_pattern": "choice_clean_tests_{model}.jsonl",
          "metrics": {
            "clean_rate": "choice_rates.clean",
            "flag_rate": "choice_rates.flag",
            "hack_rate": "choice_rates.hack"
          }
        }
      ],
      "y_limit": [0, 1]
    },
    {
      "title": "Flag Rates Comparison",
      "position": [1, 0],
      "x_labels": ["broken_tests", "clean_tests"],
      "metrics": [
        {
          "name": "flag_rate",
          "label": "flag rate",
          "style": {"alpha": 0.7, "hatch": "//"}
        }
      ],
      "data_sources": [
        {
          "x_index": 0,
          "file_pattern": "choice_broken_tests_{model}.jsonl",
          "metrics": {
            "flag_rate": "choice_rates.flag"
          }
        },
        {
          "x_index": 1,
          "file_pattern": "choice_clean_tests_{model}.jsonl",
          "metrics": {
            "flag_rate": "choice_rates.flag"
          }
        }
      ],
      "y_limit": [0, 1]
    },
    {
      "title": "Hack Rates Comparison",
      "position": [1, 1],
      "x_labels": ["broken_tests", "clean_tests"],
      "metrics": [
        {
          "name": "hack_rate",
          "label": "hack rate",
          "style": {"alpha": 0.7, "hatch": "\\\\"}
        }
      ],
      "data_sources": [
        {
          "x_index": 0,
          "file_pattern": "choice_broken_tests_{model}.jsonl",
          "metrics": {
            "hack_rate": "choice_rates.hack"
          }
        },
        {
          "x_index": 1,
          "file_pattern": "choice_clean_tests_{model}.jsonl",
          "metrics": {
            "hack_rate": "choice_rates.hack"
          }
        }
      ],
      "y_limit": [0, 1]
    }
  ]
}